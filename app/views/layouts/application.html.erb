<!DOCTYPE html>
<html>
  <head>
    <title>SelectraExercise</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    
  </head>

  <body>
    <header>
      <% if user_signed_in? %>
        <%= render "users/header-loggedin" %>
      <% end %>
    </header>
    <%= yield %>
  </body>
</html>
