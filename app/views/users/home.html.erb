<% if user_signed_in? %>
  <div class="container">
    <div class="row">
      <img src="selectra.png" alt="selectra">
    </div>
    
    <div class="row">
      
      <a href="/users" class="button1">List of Users</a>
    </div>
    <div class="row">
      <a href="/users/<%=current_user.id%>" class="button1">My Profile</a>
    </div>
    <div class="row">
      
      <a href="/locations" class="button1">List of Locations</a>
    </div>
    

  </div>
<% else %>
  <div class="container">
    <div class="row">
      <img src="selectra.png" alt="selectra">
    </div>
    
    <div class="row">
      
      <a href="/users/sign_in">Log In!</a>
    </div>
    <div class="row">
      
      <a href="/users/sign_up">Register</a>
    </div>
  </div>
  
<% end %>


    

  