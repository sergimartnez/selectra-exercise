<div class="container">
  <div class="row">
    <div class="col-sm-4">
      <h4>Email</h4>
    </div>
    <div class="col-sm-4">
      <h4>Name</h4>
    </div>
  </div>
  <% @users.each do |user| %>
    <div class="row">
      <div class="col-sm-4">
        <h4><%= user.email %></h4>
      </div>
      <div class="col-sm-4">
        <h4><%= user.name %></h4>
      </div>
      <!-- <h4><%= link_to user.name, "/user/#{user.id}" %></h4> -->
    </div>
  <% end %>
</div>